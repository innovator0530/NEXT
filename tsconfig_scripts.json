{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "preserve",
		"paths": {
			"@lib/*": ["./lib/*"],
			"@middleware/*": ["./middleware/*"],
			"@middleware": ["./middleware/index"],
			"@models/*": ["./models/*"],
			"@pages/*": ["./pages/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/register.jsx"],
	"exclude": ["node_modules"],
	"ts-node":{
		"files":true
	}
}
