{
	"name": "rewave",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"next": "next",
		"dev": "node utils/startapp",
		"start:alpha": "node utils/startapp alpha",
		"start:beta": "node utils/startapp beta",
		"start:live": "node utils/startapp live",
		"deploy:alpha": "sh _docker/deploy-alpha.sh",
		"deploy:beta": "sh _docker/deploy-beta.sh",
		"deploy:production": "sh _docker/deploy-production.sh",
		"nextbuild": "NODE_OPTIONS=--max-old-space-size=4096 next build",
		"build": "npm run init-checks:prod && next build",
		"start": "next start",
		"build:beta": "cp .env.production-beta .env.production && npm run init-checks:prod  && npm run build",
		"build:alpha": "cp .env.production-alpha .env.production && npm run init-checks:prod  && npm run build",
		"static": "next build && next export",
		"transfer": "rsync --progress -az --delete ./ --exclude '.env' --exclude 'node_modules' rewave-ch-digitalocean:/home/rewave/rewave",
		"deploy": "yarn build && yarn transfer && ssh -t rewave-ch-digitalocean 'cd /home/rewave/rewave && yarn install --production && pm2 reload rewave-frontend && exit'",
		"transfer-demo": "rsync -azv --exclude node_modules/ --exclude .git --exclude .next --exclude .temp --exclude public/mocks/ . rewave-testing:/home/ubuntu/rewave-testing",
		"sync-alpha": "rsync -azv --exclude node_modules/ --exclude .git --exclude .next --exclude .temp --exclude public/mocks/ . rewave-testing:/home/ubuntu/rewave-alpha",
		"sync-beta": "rsync -azv --exclude node_modules/ --exclude .git --exclude .next --exclude .temp --exclude public/mocks/ . rewave-testing:/home/ubuntu/rewave-beta",
		"start-test": "next start -p 3142",
		"start-mongodb": "sudo systemctl start mongod",
		"stop-mongodb": "sudo systemctl stop mongod",
		"test": "jest --watch",
		"json-schema": "typescript-json-schema  ./models/api/PostRelease.model.ts PostRelease  --required -o models/api/json-schema/PostRelease.json",
		"mongodump": "mongodump --db rewave --out utils/mongodump",
		"mongorestore": "mongorestore utils/mongodump",
		"postinstall": "patch-package",
		"cypress": "cypress open",
		"ts-node-script": "ts-node --project tsconfig_scripts.json -r tsconfig-paths/register",
		"init-checks:dev": "npm run ts-node-script utils/init-checks.ts .env.development.local",
		"init-checks:prod": "npm run ts-node-script utils/init-checks.ts .env.production",
		"set-first-rewave-id:dev": "npm run ts-node-script utils/set-first-rewave-id.ts .env.development.local",
		"set-first-rewave-id:prod": "npm run ts-node-script utils/set-first-rewave-id.ts .env.production",
		"set-first-rewave-id:prod-alpha": "npm run ts-node-script utils/set-first-rewave-id.ts .env.production-alpha",
		"set-first-rewave-id:prod-beta": "npm run ts-node-script utils/set-first-rewave-id.ts .env.production-beta",
		"set-first-isrc:dev": "npm run ts-node-script utils/set-first-isrc.ts .env.development.local",
		"set-first-isrc:prod": "npm run ts-node-script utils/set-first-isrc.ts .env.production",
		"set-first-isrc:prod-alpha": "npm run ts-node-script utils/set-first-isrc.ts .env.production-alpha",
		"set-first-isrc:prod-beta": "npm run ts-node-script utils/set-first-isrc.ts .env.production-beta"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.33.0",
		"@aws-sdk/s3-request-presigner": "^3.33.0",
		"@types/mongoose": "^5.11.97",
		"ajv": "^8.6.3",
		"ajv-formats": "^2.1.1",
		"aws-sdk": "^2.1048.0",
		"bcrypt": "^5.0.1",
		"cross-fetch": "^3.1.4",
		"form-data-encoder": "^1.7.1",
		"formdata-node": "^4.3.1",
		"formidable": "^1.2.2",
		"mongoose": "^5.13.9",
		"next": "^12.0.8",
		"next-auth": "^4.0.6",
		"next-connect": "^0.10.2",
		"next-iron-session": "^4.2.0",
		"node-cron": "^3.0.0",
		"nodemailer": "^6.6.5",
		"react": "^17.0.2",
		"react-beautiful-dnd": "^13.1.0",
		"react-datepicker": "^3.8.0",
		"react-dom": "^17.0.2",
		"sharp": "^0.28.3",
		"styled-components": "^5.3.1",
		"swr": "^0.5.7",
		"uuid": "^8.3.2",
		"xlsx": "^0.17.4"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-typescript": "^7.15.0",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@testing-library/cypress": "^8.0.2",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@types/node": "^14.17.18",
		"@types/nodemailer": "^6.4.4",
		"@types/react": "^17.0.24",
		"babel-jest": "^27.4.6",
		"cypress": "^9.3.1",
		"dotenv": "^10.0.0",
		"husky": "^5.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.4.7",
		"jest-node-exports-resolver": "^1.1.4",
		"patch-package": "^6.4.7",
		"react-test-renderer": "^17.0.2",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.12.0",
		"typescript": "^4.4.3",
		"typescript-json-schema": "^0.51.0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	}
}
